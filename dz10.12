#include <stdio.h>
#include <iostream>
#include <string>

using namespace std;

int main(){
    string s1, s2, s;
    int k;
    cin >> s >> s1 >> s2;
    string rez = "";
    
    for (int i = 0; i <= s.length() - s1.length(); i++){
        bool b = true;
        for (int j = 0; j < s1.length(); j++){
            if (s[i + j] != s1[j])
            {
                b = false;
                break;
            }
        }
        if (b){
            rez += s2;
            i += s1.length() - 1;
        }
        else 
        {
            rez += s[i];
        }
    }

    cout << rez;
}
