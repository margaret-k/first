#include <stdio.h>
#include <iostream>
#include <string>
#include <cmath>

using namespace std;

int main(){
    int n;
    cin >> n;
    int matrix[n][n];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> matrix[n - i - 1][n - j - 1];

    cout << endl;
    
    for (int i = 0; i < n; i++){
        for (int j = 0; j < n; j++)
            cout << matrix[i][j] << ' ';
        cout << endl;
    }
}
