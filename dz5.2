#include <stdio.h>
#include <iostream>
#include <cstdint>

using namespace std;

int main(){
	unsigned int m, n;
	cin >> m >> n;

    int same = 0;
    int positive = 0;

    for (int i = 0; i < sizeof(m) * 8; i++){
		if ((m & (1 << i)) == (n & (1 << i)))
        {
            ++same;
            if ((m & (1 << i)) != 0)
                ++positive;
        }
	}

    cout << positive << ' ' << same << endl;
}
